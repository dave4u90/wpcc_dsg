			<tr>
				<td> <%= link_to component.component_type_description, component %></td>
			</tr>
			<% end %> 
		</table> 
	</div> 
</div>
<div class="clearfix"></div>
<div style="width: 100%;height:40px;padding-top: 15px;position: relative" class="row-fluid">
	<div class="pull-right" style="padding-right: 10px">
		<%= link_to "Return to Products", product_instances_path, :class=>"btn btn-success", :style=>"padding-left:20px;padding-right:20px"%>
	</div>
	
</div>

